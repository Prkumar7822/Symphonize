package java_training;

public class Sub_polindrome {
	public static void main(String[] args) {
		String s="bob";
		String s1="";
		for(int i=0;i<s.length();i++) {
			char c=s.charAt(i);
			s1=c+s1;
			
		
		
		if(s.equals(s1)) {
//			System.out.println("Polindrome");
		}
		else {
//			System.out.println("NOt polindrome");
		}
	}
	

}
