package java_training;

public class s3Sum {

	public static void main(String[] args) {
		
		int arr[]= {1,-1,0,2,-2};
		
		for(int i=0;i<arr.length;i++) {
			int sum=0;
			for(int j=0;j<arr.length;j++) {
				for(int k=i;k<=j;k++) {
					sum+=arr[k];
					if(sum==0) {
						System.out.print(arr[k]);
					}
				}
			}
		}
		
		

	}

}
